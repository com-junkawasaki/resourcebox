{
  "name": "@gftdcojp/resourcebox",
  "version": "0.2.1",
  "description": "TypeBox-inspired RDF Resource type builder with SHACL validation and OWL ontology support for TypeScript",
  "license": "Apache-2.0",
  "author": "GFTD Co., JP",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "resourcebox": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./resource": {
      "import": "./dist/resource/index.js",
      "types": "./dist/resource/index.d.ts"
    },
    "./onto": {
      "import": "./dist/onto/index.js",
      "types": "./dist/onto/index.d.ts"
    },
    "./shape": {
      "import": "./dist/shape/index.js",
      "types": "./dist/shape/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "src", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "clean": "rm -rf dist coverage",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "docs:start": "pnpm --dir docs-site start",
    "docs:build": "pnpm --dir docs-site build",
    "docs:serve": "pnpm --dir docs-site serve",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test && pnpm lint"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.35",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^14.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "2.1.9",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "rdf",
    "resource",
    "owl",
    "shacl",
    "shex",
    "json-ld",
    "typebox",
    "validation",
    "semantic-web",
    "ontology",
    "typescript",
    "type-safety",
    "resourcebox"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/com-junkawasaki/resourcebox.git"
  },
  "homepage": "https://github.com/com-junkawasaki/resourcebox#readme",
  "bugs": {
    "url": "https://github.com/com-junkawasaki/resourcebox/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}
